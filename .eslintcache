?.setErrors({});\n\n        const schema = Yup.object().shape({\n          email: Yup.string()\n            .required('E-mail obrigatório')\n            .email('Digite um e-mail válido'),\n          password: Yup.string().min(8, 'Senha obrigatória'),\n        });\n\n        await schema.validate(data, { abortEarly: false });\n\n        signIn({\n          email: data.email,\n          password: data.password,\n        });\n      } catch (error) {\n        const errors = getValidationErrors(error);\n        formRef.current?.setErrors(errors);\n      }\n    },\n    [signIn],\n  );\n\n  return (\n    <Container>\n      <Content>\n        <img src={logo} alt=\"GoBarber\" />\n\n        <Form ref={formRef} onSubmit={handleSubmit}>\n          <h1>Faça seu logon</h1>\n          <Input icon={FiMail} name=\"email\" type=\"text\" placeholder=\"E-mail\" />\n          <Input\n            icon={FiLock}\n            name=\"password\"\n            type=\"password\"\n            placeholder=\"Password\"\n          />\n\n          <Button type=\"submit\">Entrar</Button>\n\n          <a href=\"forgot-password\">Esqueci minha senha</a>\n        </Form>\n        <a href=\"forgot-password\">\n          <FiLogIn />\n          Criar conta\n        </a>\n      </Content>\n\n      <Background />\n    </Container>\n  );\n};\n\nexport default SignIn;\n","C:\\Users\\hiago\\workspace\\Rocktseat\\go-stack\\projects\\gobarber-web\\src\\components\\Input\\index.tsx",["71"],"import React, {\n  InputHTMLAttributes,\n  useEffect,\n  useRef,\n  useState,\n  useCallback,\n} from 'react';\nimport { IconBaseProps } from 'react-icons';\nimport { FiAlertCircle } from 'react-icons/fi';\nimport { useField } from '@unform/core';\n\nimport Tooltip from '../Tooltip';\n\nimport { Container, Error } from './styles';\n\ninterface InputProps extends InputHTMLAttributes<HTMLInputElement> {\n  name: string;\n  icon: React.ComponentType<IconBaseProps>;\n}\n\nconst Input: React.FC<InputProps> = ({ name, icon: Icon, ...rest }) => {\n  const inputRef = useRef<HTMLInputElement>(null);\n  const [isFocused, setIsFocused] = useState(false);\n  const [isFilled, setIsFilled] = useState(false);\n  const { fieldName, defaultValue, error, registerField } = useField(name);\n\n  const handleInputFocus = useCallback(() => {\n    setIsFocused(true);\n  }, []);\n\n  const handleInputBlur = useCallback(() => {\n    setIsFocused(false);\n\n    setIsFilled(!!inputRef.current?.value);\n  }, []);\n\n  useEffect(() => {\n    registerField({ name: fieldName, ref: inputRef.current, path: 'value' });\n  }, [fieldName, registerField]);\n\n  return (\n    <Container isErrored={!!error} isFilled={isFilled} isFocused={isFocused}>\n      {Icon && <Icon />}\n      <input\n        autoComplete=\"false\"\n        onFocus={handleInputFocus}\n        onBlur={handleInputBlur}\n        defaultValue={defaultValue}\n        ref={inputRef}\n        {...rest}\n      />\n      {error && (\n        <Error title={error}>\n          <FiAlertCircle color=\"#c53030\" size={20} />\n        </Error>\n      )}\n    </Container>\n  );\n};\n\nexport default Input;\n","C:\\Users\\hiago\\workspace\\Rocktseat\\go-stack\\projects\\gobarber-web\\src\\pages\\SignUp\\styles.ts",[],"C:\\Users\\hiago\\workspace\\Rocktseat\\go-stack\\projects\\gobarber-web\\src\\pages\\SignUp\\index.tsx",[],"C:\\Users\\hiago\\workspace\\Rocktseat\\go-stack\\projects\\gobarber-web\\src\\components\\Button\\index.tsx",[],"C:\\Users\\hiago\\workspace\\Rocktseat\\go-stack\\projects\\gobarber-web\\src\\components\\Button\\styles.ts",[],"C:\\Users\\hiago\\workspace\\Rocktseat\\go-stack\\projects\\gobarber-web\\src\\components\\Input\\styles.ts",[],"C:\\Users\\hiago\\workspace\\Rocktseat\\go-stack\\projects\\gobarber-web\\src\\utils\\getValidationErros.ts",[],"C:\\Users\\hiago\\workspace\\Rocktseat\\go-stack\\projects\\gobarber-web\\src\\components\\Tooltip\\styles.ts",[],"C:\\Users\\hiago\\workspace\\Rocktseat\\go-stack\\projects\\gobarber-web\\src\\components\\To